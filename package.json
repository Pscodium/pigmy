{
  "name": "pigmy",
  "version": "1.0.6",
  "description": "A simple and expressive terminal color library for Node.js.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "imports": {
    "#ansi": {
      "codes": "./lib/utils/ansi/ansi.js",
      "default": "./lib/utils/ansi/index.js"
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsup lib/index.js --format cjs,esm --out-dir dist && node ./scripts/copy-dts.cjs",
    "copy:dts": "node ./scripts/copy-dts.cjs",
    "prepare": "npm run build"
  },
  "keywords": [
    "color",
    "terminal",
    "cli",
    "console",
    "pigmy",
    "ansi",
    "style",
    "text"
  ],
  "author": "Pscodium <peterson@pscodium.dev>",
  "license": "MIT",
  "files": ["dist"],
  "sideEffects": false,
  "devDependencies": {
    "tsup": "^6.7.0"
  }
}
